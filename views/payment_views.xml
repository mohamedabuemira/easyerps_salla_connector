<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_payment_method_view_tree" model="ir.ui.view">
        <field name="name">account_payment_method.view.tree</field>
        <field name="model">account.payment.method</field>
        <field name="arch" type="xml">
            <tree string="" editable="bottom">
                <field name="salla_id" />
                <field name="name" />
                <field name="code" />
                <field name="payment_type" />
            </tree>
        </field>
    </record>
<record id="account_payment_method_action" model="ir.actions.act_window">
    <field name="name">Payment Method</field>
    <field name="res_model">account.payment.method</field>
    <field name="view_mode">tree</field>
</record>
</odoo>
